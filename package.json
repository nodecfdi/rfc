{
  "name": "@nodecfdi/rfc",
  "description": "Librer√≠a para trabajar con RFC (Mexicano)",
  "version": "2.0.4",
  "type": "module",
  "main": "",
  "module": "",
  "types": "",
  "files": [
    "dist"
  ],
  "exports": {},
  "imports": {
    "#src/*": "./src/*.js"
  },
  "scripts": {
    "prepare": "is-in-ci || husky",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "gen:docs": "typedoc --options typedoc.json",
    "changelog": "auto-changelog -p && git add CHANGELOG.md",
    "prebuild": "pnpm lint:check && pnpm typecheck",
    "build": "tshy",
    "postbuild": "pnpm run gen:docs && git add docs/*",
    "release": "np",
    "version": "pnpm run build && pnpm run changelog"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@jsprismarine/typedoc-material-theme": "^1.0.4",
    "@nodecfdi/eslint-config": "^2.0.2",
    "@nodecfdi/prettier-config": "^1.2.0",
    "@nodecfdi/tsconfig": "^1.7.2",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.16.5",
    "@vitest/coverage-istanbul": "^2.0.5",
    "auto-changelog": "^2.5.0",
    "eslint": "^9.10.0",
    "husky": "^9.1.5",
    "is-in-ci": "^1.0.0",
    "luxon": "^3.4.4",
    "prettier": "^3.3.3",
    "tshy": "^3.0.2",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "peerDependencies": {
    "@types/luxon": "3.4.2",
    "luxon": "^3.4.4"
  },
  "peerDependenciesMeta": {
    "@types/luxon": {
      "optional": true
    }
  },
  "author": "Fernando Isidro <luffynando@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nodecfdi/rfc",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodecfdi/rfc.git"
  },
  "bugs": {
    "url": "https://github.com/nodecfdi/rfc/issues"
  },
  "keywords": [
    "rfc",
    "mexican",
    "homoclave"
  ],
  "engines": {
    "node": ">=18 <=22 || ^16"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": "@nodecfdi/prettier-config",
  "packageManager": "pnpm@9.10.0",
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "auto-changelog": {
    "template": "conventional",
    "hideCredit": true
  },
  "np": {
    "message": "chore(release): :tada: %s",
    "tag": "latest",
    "branch": "main",
    "testScript": "test:run"
  },
  "tshy": {
    "selfLink": false,
    "exports": {
      ".": "./src/index.ts",
      "./errors": "./src/errors.ts",
      "./package.json": "./package.json"
    }
  }
}
